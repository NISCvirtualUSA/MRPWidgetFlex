<configuration label="Mission Ready Packages">
	<appname>MRP Demonstration</appname>
    <layer></layer>
    <query></query>
    <titlefield>Name</titlefield>
    <linkfield>URL</linkfield>
    <fields all="true">
        <field name="Name" alias="MRP Name: " visible="true"/>
        <field name="MRPCatalogType" alias="MRP Type: " visible="true"/>
        <field name="NIMSCategory" alias="NIMS Category: " visible="true"/>
        <field name="NIMSType"   alias="NIMS Type" visible="true"/>       
		<field name="MissionCapabilities" alias="Mission Capabilities" visible="true"/>
		<field name="MRPStatus" alias="MRP Status: " visible="true"/>
		<field name="EstimatedFixedCost"   alias="Est Fixed Cost" visible="true"/>
		<field name="CostNotes"   alias="Cost Notes" visible="true"/>
		<field name="POCLastName"   alias="POC Last Name" visible="true"/>
		<field name="POCFirstName" alias="POC First Name" visible="true"/>
		<field name="POCEmail" alias="POC Email" visible="true"/>
		<field name="POCPhone" alias="POC Phone" visible="true"/>
		<field name="EMACLastName"   alias="EMAC Last Name" visible="true"/>
		<field name="EMACFirstName" alias="EMAC First Name" visible="true"/>
		<field name="EMACEmail" alias="EMAC Email" visible="true"/>
		<field name="EMACPhone" alias="EMAC Phone" visible="true"/>
		<field name="URL" alias="URL"/>
    </fields>
    
 	  <mrpstatus_field>MRPStatus</mrpstatus_field><!--  use this for the name search -->
	  <mrpcatalogtype_field>MRPCatalogType</mrpcatalogtype_field><!-- this is used for getting sector icons/DOW icons and requires values such as Monday, Tuesday. -->
	  <mrpestfixedcost_field>EstimatedFixedCost</mrpestfixedcost_field>
	  <nimscategory_field>NIMSCategory</nimscategory_field>
	  <mrpstate_field>State</mrpstate_field>
  
    
    <!--
    	Not enabled <refreshrate></refreshrate> 
    -->
    
    <zoomscale>80000</zoomscale>
</configuration>

